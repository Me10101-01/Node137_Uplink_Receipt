# UTILITY FUNCTIONS
# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

def get_size_str(size_bytes: int) -> str:
    """Convert bytes to human readable string."""
    for unit in ['B', 'KB', 'MB', 'GB', 'TB']:
        if size_bytes < 1024:
            return f"{size_bytes:.2f} {unit}"
        size_bytes /= 1024
    return f"{size_bytes:.2f} PB"

def get_drive_space(drive: str) -> Tuple[int, int, int]:
    """Get drive space: (total, used, free) in bytes."""
    try:
        total, used, free = shutil.disk_usage(drive)
        return total, used, free
    except Exception as e:
        print(f"[ERROR] Cannot access {drive}: {e}")
        return 0, 0, 0

def hash_file(filepath: str, block_size: int = 65536) -> str:
    """Generate SHA256 hash of file for verification."""
    hasher = hashlib.sha256()
    try:
        with open(filepath, 'rb') as f:
            for block in iter(lambda: f.read(block_size), b''):
                hasher.update(block)
        return hasher.hexdigest()[:16]  # Short hash for display
    except:
        return "NO_HASH"

def call_ollama(prompt: str, model: str = None) -> str:
    """Call Ollama for AI-assisted decision making."""
    model = model or CONFIG["ollama_model"]
    try:
        result = subprocess.run(
            ["ollama", "run", model, prompt],
            capture_output=True,
            text=True,
            timeout=120
        )
        return result.stdout.strip()
    except FileNotFoundError:
        return "[Ollama not found - install with: curl -fsSL https://ollama.com/install.sh | sh]"
    except subprocess.TimeoutExpired:
        return "[Ollama timeout - model may be loading]"
    except Exception as e:
        return f"[Ollama error: {e}]"

# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
# SCANNER CLASS
# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

class SovereignScanner:
    """
    FlameLang-integrated file scanner for emergency triage.
    Glyph Binding: RC1 [950] - Recon Init
    """
    
    def __init__(self):
        self.files: List[Dict] = []
        self.stats = defaultdict(lambda: {"count": 0, "size": 0})
        self.large_files: List[Dict] = []
        self.deletable_files: List[Dict] = []
        self.critical_files: List[Dict] = []
        self.scan_time = None
        
    def scan(self, paths: List[str] = None) -> None:
        """Execute RC1 - Recon Init scan."""
        print("\n" + "â•" * 70)
        print("  ğŸ”¥ GLYPH [RC1] RECON INIT - Scanning filesystem...")
        print("â•" * 70 + "\n")
        
        paths = paths or CONFIG["scan_paths"]
        self.scan_time = datetime.now()
        
        for scan_path in paths:
            if not os.path.exists(scan_path):
                print(f"  [SKIP] Path not found: {scan_path}")
                continue
                
            print(f"  [SCAN] {scan_path}")
            self._scan_directory(scan_path)
        
        self._categorize_files()
        print(f"\n  [DONE] Scanned {len(self.files)} files")
        
    def _scan_directory(self, path: str) -> None:
        """Recursively scan directory."""
        try:
            for root, dirs, files in os.walk(path):
                # Skip excluded patterns
                if any(ex in root for ex in CONFIG["exclude_patterns"]):
                    continue
                    
                for filename in files:
                    try:
                        filepath = os.path.join(root, filename)
                        stat = os.stat(filepath)
                        ext = os.path.splitext(filename)[1].lower()
                        
                        file_info = {
                            "path": filepath,
                            "name": filename,
                            "ext": ext,
                            "size": stat.st_size,
                            "modified": datetime.fromtimestamp(stat.st_mtime),
                            "priority": self._get_priority(ext),
                        }
                        
                        self.files.append(file_info)
                        self.stats[ext]["count"] += 1
                        self.stats[ext]["size"] += stat.st_size
                        
                    except (PermissionError, OSError):
                        continue
                        
        except (PermissionError, OSError) as e:
            print(f"  [WARN] Cannot access: {path} - {e}")
            
    def _get_priority(self, ext: str) -> str:
        """Determine file priority based on extension."""
        for priority, extensions in CONFIG["priority_extensions"].items():
            if ext in extensions:
                return priority
        return "unknown"
        
    def _categorize_files(self) -> None:
        """Categorize files after scan."""
        threshold_bytes = CONFIG["large_file_threshold_mb"] * 1024 * 1024
        
        for f in self.files:
            if f["size"] > threshold_bytes:
                self.large_files.append(f)
            if f["priority"] == "deletable":
                self.deletable_files.append(f)
            if f["priority"] == "critical":
                self.critical_files.append(f)
                
        # Sort by size descending
        self.large_files.sort(key=lambda x: x["size"], reverse=True)
        self.deletable_files.sort(key=lambda x: x["size"], reverse=True)

# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
# ANALYZER CLASS
# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

class SovereignAnalyzer:
    """
    FlameLang-integrated file analyzer for triage decisions.
    Glyph Binding: RC2 [951] - Recon Scan
    """
    
    def __init__(self, scanner: SovereignScanner):
        self.scanner = scanner
        self.report = {}
        
    def analyze(self) -> Dict:
        """Execute RC2 - Recon Scan analysis."""
        print("\n" + "â•" * 70)
        print("  ğŸ”¥ GLYPH [RC2] RECON SCAN - Analyzing data...")
        print("â•" * 70 + "\n")
        
        # Drive status
        c_total, c_used, c_free = get_drive_space("C:\\")
        g_total, g_used, g_free = get_drive_space("G:\\")
        d_total, d_used, d_free = get_drive_space("D:\\")
        
        self.report = {
            "timestamp": datetime.now().isoformat(),
            "drives": {
                "C:": {
                    "name": "Acer (System)",
                    "total": c_total,
                    "free": c_free,
                    "used_pct": (c_used / c_total * 100) if c_total > 0 else 0,
                    "status": "CRITICAL" if c_free < CONFIG["critical_threshold_gb"] * 1024**3 else "OK"
                },
                "G:": {
                    "name": "ERIS_CORE",
                    "total": g_total,
                    "free": g_free,
                    "used_pct": (g_used / g_total * 100) if g_total > 0 else 0,
                    "status": "READY"
                },
                "D:": {
                    "name": "PS5_VAULT",
                    "total": d_total,
                    "free": d_free,
                    "used_pct": (d_used / d_total * 100) if d_total > 0 else 0,
                    "status": "READY"
                },
            },
            "scan_summary": {
                "total_files": len(self.scanner.files),
                "total_size": sum(f["size"] for f in self.scanner.files),
                "large_files": len(self.scanner.large_files),
                "deletable_files": len(self.scanner.deletable_files),
                "critical_files": len(self.scanner.critical_files),
            },
            "immediate_actions": self._generate_actions(),
            "top_large_files": [
                {
                    "path": f["path"],
                    "size": get_size_str(f["size"]),
                    "priority": f["priority"]
                }
                for f in self.scanner.large_files[:20]
            ],
            "deletable_space": sum(f["size"] for f in self.scanner.deletable_files),
        }
        
        return self.report
        
    def _generate_actions(self) -> List[str]:
        """Generate recommended immediate actions."""
        actions = []
        
        # Check for deletable files
        deletable_size = sum(f["size"] for f in self.scanner.deletable_files)
        if deletable_size > 100 * 1024 * 1024:  # > 100 MB
            actions.append(f"DELETE: {get_size_str(deletable_size)} of temp/cache files")
            
        # Check for large files to migrate
        large_size = sum(f["size"] for f in self.scanner.large_files)
        if large_size > 1024 * 1024 * 1024:  # > 1 GB
            actions.append(f"MIGRATE: {get_size_str(large_size)} of large files to ERIS_CORE")
            
        # Check for VDI/VM files
        vm_files = [f for f in self.scanner.files if f["ext"] in [".vdi", ".vmdk", ".iso"]]
        if vm_files:
            vm_size = sum(f["size"] for f in vm_files)
            actions.append(f"MIGRATE VMs: {get_size_str(vm_size)} of virtual machines to ERIS_CORE")
            
        # Check for video files
        video_files = [f for f in self.scanner.files if f["ext"] in [".mp4", ".mkv", ".avi", ".mov"]]
        if video_files:
            video_size = sum(f["size"] for f in video_files)
            actions.append(f"MIGRATE MEDIA: {get_size_str(video_size)} of video files to PS5_VAULT")
            
        return actions
        
    def print_report(self) -> None:
        """Print formatted analysis report."""
        print("\n" + "â•" * 70)
        print("  ğŸ“Š TRIAGE REPORT")
        print("â•" * 70)
        
        # Drive status
        print("\n  DRIVE STATUS:")
        print("  " + "-" * 66)
        for drive, info in self.report["drives"].items():
            status_icon = "ğŸ”´" if info["status"] == "CRITICAL" else "ğŸŸ¢"
            print(f"  {status_icon} {drive} {info['name']}")
            print(f"      Free: {get_size_str(info['free'])} / {get_size_str(info['total'])}")
            print(f"      Used: {info['used_pct']:.1f}%")
            print(f"      Status: {info['status']}")
            print()
            
        # Scan summary
        print("  SCAN SUMMARY:")
        print("  " + "-" * 66)
        summary = self.report["scan_summary"]
        print(f"      Total Files: {summary['total_files']:,}")
        print(f"      Total Size: {get_size_str(summary['total_size'])}")
        print(f"      Large Files (>{CONFIG['large_file_threshold_mb']}MB): {summary['large_files']}")
        print(f"      Deletable Files: {summary['deletable_files']}")
        print(f"      Critical Files: {summary['critical_files']}")
        print()
        
        # Immediate actions
        print("  ğŸš¨ IMMEDIATE ACTIONS:")
        print("  " + "-" * 66)
        for i, action in enumerate(self.report["immediate_actions"], 1):
            print(f"      {i}. {action}")
        print()
        
        # Top large files
        print("  ğŸ“¦ TOP 10 LARGEST FILES:")
        print("  " + "-" * 66)
        for i, f in enumerate(self.report["top_large_files"][:10], 1):
            print(f"      {i}. [{f['priority'].upper()}] {f['size']}")
            print(f"         {f['path']}")
        print()
        
        # Deletable space
        print(f"  ğŸ—‘ï¸  RECOVERABLE SPACE: {get_size_str(self.report['deletable_space'])}")
        print()

# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
# MIGRATOR CLASS
# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

class SovereignMigrator:
    """
    FlameLang-integrated file migrator for emergency evacuation.
    Glyph Binding: FB1 [137] - Flamebearer (Protect)
    """
    
    def __init__(self, scanner: SovereignScanner):
        self.scanner = scanner
        self.migrated = []
        self.failed = []
        
    def generate_migration_plan(self) -> Dict:
        """Generate migration plan without executing."""
        print("\n" + "â•" * 70)
        print("  ğŸ”¥ GLYPH [FB1] FLAMEBEARER - Generating protection plan...")
        print("â•" * 70 + "\n")
        
        plan = {
            "to_eris_core": [],  # G: - Large files, VMs
            "to_ps5_vault": [],  # D: - Media files
            "to_delete": [],     # Temp/cache files
            "to_backup": [],     # Critical code files
        }
        
        for f in self.scanner.files:
            if f["priority"] == "deletable":
                plan["to_delete"].append(f)
            elif f["ext"] in [".vdi", ".vmdk", ".iso"]:
                plan["to_eris_core"].append(f)
            elif f["ext"] in [".mp4", ".mkv", ".avi", ".mov", ".mp3", ".wav"]:
                plan["to_ps5_vault"].append(f)
            elif f["priority"] == "critical":
                plan["to_backup"].append(f)
            elif f["size"] > CONFIG["large_file_threshold_mb"] * 1024 * 1024:
                plan["to_eris_core"].append(f)
                
        return plan
        
    def execute_migration(self, plan: Dict, dry_run: bool = True) -> None:
        """Execute migration plan."""
        print("\n" + "â•" * 70)
        if dry_run:
            print("  ğŸ”¥ DRY RUN - No files will be moved")
        else:
            print("  ğŸ”¥ EXECUTING MIGRATION - Files will be moved!")
        print("â•" * 70 + "\n")
        
        # Migrate to ERIS_CORE
        if plan["to_eris_core"]:
            target_dir = "G:\\Sovereign_Migration\\from_C"
            print(f"  [MIGRATE] {len(plan['to_eris_core'])} files â†’ {target_dir}")
            if not dry_run:
                os.makedirs(target_dir, exist_ok=True)
            for f in plan["to_eris_core"][:10]:  # Limit for safety
                print(f"    â†’ {f['name']} ({get_size_str(f['size'])})")
                if not dry_run:
                    self._move_file(f["path"], target_dir)
                    
        # Migrate to PS5_VAULT
        if plan["to_ps5_vault"]:
            target_dir = "D:\\Media_Migration"
            print(f"\n  [MIGRATE] {len(plan['to_ps5_vault'])} files â†’ {target_dir}")
            if not dry_run:
                os.makedirs(target_dir, exist_ok=True)
            for f in plan["to_ps5_vault"][:10]:
                print(f"    â†’ {f['name']} ({get_size_str(f['size'])})")
                if not dry_run:
                    self._move_file(f["path"], target_dir)
                    
        # Delete temp files
        if plan["to_delete"]:
            total_delete = sum(f["size"] for f in plan["to_delete"])
            print(f"\n  [DELETE] {len(plan['to_delete'])} temp files ({get_size_str(total_delete)})")
            if not dry_run:
                for f in plan["to_delete"]:
                    self._delete_file(f["path"])
                    
        print("\n  [COMPLETE] Migration plan executed")
        
    def _move_file(self, source: str, target_dir: str) -> bool:
        """Move file with verification."""
        try:
            filename = os.path.basename(source)
            target = os.path.join(target_dir, filename)
            
            # Hash before
            hash_before = hash_file(source)
            
            # Move
            shutil.move(source, target)
            
            # Hash after
            hash_after = hash_file(target)
            
            if hash_before == hash_after:
                self.migrated.append({"source": source, "target": target, "hash": hash_before})
                return True
            else:
                self.failed.append({"source": source, "error": "Hash mismatch"})
                return False
                
        except Exception as e:
            self.failed.append({"source": source, "error": str(e)})
            return False
            
    def _delete_file(self, filepath: str) -> bool:
        """Delete file safely."""
        try:
            os.remove(filepath)
            return True
        except Exception as e:
            self.failed.append({"source": filepath, "error": str(e)})
            return False

# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
# OLLAMA INTEGRATION
# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

class OllamaTriage:
    """
    AI-assisted triage using Ollama.
    Glyph Binding: GR1 [999] - Glyphos Resonance (Full Cascade)
    """
    
    def __init__(self, analyzer: SovereignAnalyzer):
        self.analyzer = analyzer
        self.model = CONFIG["ollama_model"]
        
    def get_ai_recommendations(self) -> str:
        """Get AI-powered triage recommendations."""
        print("\n" + "â•" * 70)
        print(f"  ğŸ”¥ GLYPH [GR1] GLYPHOS RESONANCE - Consulting {self.model}...")
        print("â•" * 70 + "\n")
        
        # Build context prompt
        report = self.analyzer.report
        prompt = f"""You are a sovereign systems administrator for StrategicKhaos DAO.

EMERGENCY TRIAGE SITUATION:

Drive Status:
- C: (System) - {get_size_str(report['drives']['C:']['free'])} free - STATUS: {report['drives']['C:']['status']}
- G: (ERIS_CORE) - {get_size_str(report['drives']['G:']['free'])} free - STATUS: {report['drives']['G:']['status']}
- D: (PS5_VAULT) - {get_size_str(report['drives']['D:']['free'])} free - STATUS: {report['drives']['D:']['status']}

Scan Results:
- Total files scanned: {report['scan_summary']['total_files']:,}
- Total size: {get_size_str(report['scan_summary']['total_size'])}
- Large files (>100MB): {report['scan_summary']['large_files']}
- Deletable temp files: {report['scan_summary']['deletable_files']}
- Recoverable space from deletables: {get_size_str(report['deletable_space'])}

Top 5 Largest Files:
{chr(10).join(f"- {f['size']}: {f['path']}" for f in report['top_large_files'][:5])}

Provide:
1. Priority ranking of what to move first
2. What to delete immediately
3. What to keep on C: drive
4. Specific migration commands
5. Risk assessment

Be concise and tactical. This is an emergency."""

        response = call_ollama(prompt, self.model)
        print(response)
        return response

