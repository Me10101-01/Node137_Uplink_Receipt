def main():
    """Main entry point."""
    import argparse
    
    parser = argparse.ArgumentParser(
        description="Sovereign Security Scanner â€” FlameLang-Integrated Pen Testing",
        formatter_class=argparse.RawDescriptionHelpFormatter,
        epilog="""
Examples:
  sovereign-scanner 192.168.1.1              Quick scan single host
  sovereign-scanner 192.168.1.0/24           Scan network range
  sovereign-scanner example.com -f           Full port scan
  sovereign-scanner 192.168.1.1 -p 22,80,443 Scan specific ports
  sovereign-scanner 192.168.1.1 --ai         Include AI analysis
        """
    )
    
    parser.add_argument("target", help="Target IP, hostname, or CIDR range")
    parser.add_argument("-p", "--ports", help="Comma-separated ports to scan")
    parser.add_argument("-f", "--full", action="store_true", help="Full port scan (1-1000)")
    parser.add_argument("--ai", action="store_true", help="Include AI-powered analysis")
    parser.add_argument("-o", "--output", help="Output filename")
    parser.add_argument("-q", "--quiet", action="store_true", help="Minimal output")
    parser.add_argument("--version", action="version", version=f"Sovereign Scanner v{VERSION}")
    
    args = parser.parse_args()
    
    if not args.quiet:
        print_banner()
    
    # Parse ports
    ports = None
    if args.ports:
        try:
            ports = [int(p.strip()) for p in args.ports.split(",")]
        except ValueError:
            print("  [ERROR] Invalid port specification")
            sys.exit(1)
    
    # Create scanner and execute
    scanner = SovereignScanner()
    report = scanner.scan(args.target, ports=ports, full_scan=args.full)
    
    # Save report
    scanner.save_report(report, args.output)
    
    # AI analysis
    if args.ai:
        scanner.ai_analysis(report)
    
    print("\n  ğŸ”¥ Scan complete. Empire Eternal. ğŸ”¥\n")

if __name__ == "__main__":
    main()

