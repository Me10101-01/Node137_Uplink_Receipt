CODENAME = "FLAMEBEARER"

# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
# CONFIGURATION
# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

CONFIG = {
    "max_threads": 100,
    "socket_timeout": 2,
    "scan_delay": 0.01,  # Delay between scans to avoid detection
    "common_ports": [
        21, 22, 23, 25, 53, 80, 110, 111, 135, 139, 143, 443, 445, 993, 995,
        1433, 1521, 3306, 3389, 5432, 5900, 6379, 8080, 8443, 27017
    ],
    "output_dir": Path.home() / ".flamelang" / "scans",
    "ollama_model": "qwen2.5:72b",
}

# Service signatures for banner grabbing
SERVICE_SIGNATURES = {
    b"SSH": "SSH",
    b"220": "FTP/SMTP",
    b"HTTP": "HTTP",
    b"MySQL": "MySQL",
    b"PostgreSQL": "PostgreSQL",
    b"redis": "Redis",
    b"MongoDB": "MongoDB",
    b"Microsoft": "Microsoft",
    b"OpenSSH": "OpenSSH",
    b"Apache": "Apache",
    b"nginx": "Nginx",
    b"IIS": "IIS",
}

# Known vulnerability patterns
VULN_PATTERNS = {
    "OpenSSH_7.": "CVE-2016-10009 (potential)",
    "OpenSSH_6.": "Multiple CVEs (outdated)",
    "Apache/2.2": "Multiple CVEs (outdated)",
    "Apache/2.4.49": "CVE-2021-41773",
    "Apache/2.4.50": "CVE-2021-42013",
    "nginx/1.16": "CVE-2019-20372 (potential)",
    "vsftpd 2.3.4": "CVE-2011-2523 (backdoor)",
    "ProFTPD 1.3.3": "CVE-2015-3306",
    "MySQL 5.5": "Multiple CVEs (outdated)",
    "SMBv1": "CVE-2017-0144 (EternalBlue)",
}

# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
# DATA STRUCTURES
# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

@dataclass
class PortResult:
    """Result of a port scan."""
    port: int
    state: str  # open, closed, filtered
    service: str = ""
    banner: str = ""
    version: str = ""
    vulnerabilities: List[str] = field(default_factory=list)

@dataclass
class HostResult:
    """Result of a host scan."""
    ip: str
    hostname: str = ""
    state: str = "unknown"  # up, down
    os_guess: str = ""
    ports: List[PortResult] = field(default_factory=list)
    scan_time: float = 0.0

@dataclass
class ScanReport:
    """Complete scan report."""
    target: str
    start_time: datetime
    end_time: Optional[datetime] = None
    hosts: List[HostResult] = field(default_factory=list)
    summary: Dict = field(default_factory=dict)
    attestation_hash: str = ""

# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
# OLLAMA INTEGRATION
# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

class SecurityAI:
    """AI-powered security analysis using Ollama."""
    
    def __init__(self, model: str = None):
        self.model = model or CONFIG["ollama_model"]
        self.available = self._check_ollama()
    
    def _check_ollama(self) -> bool:
        """Check if Ollama is available."""
        try:
            result = subprocess.run(["ollama", "list"], capture_output=True, timeout=5)
            return result.returncode == 0
        except:
            return False
    
    def analyze_results(self, report: ScanReport) -> str:
        """AI analysis of scan results."""
        if not self.available:
            return "[Ollama not available - manual analysis required]"
        
        # Build context
        open_ports = []
        vulns = []
        for host in report.hosts:
            for port in host.ports:
                if port.state == "open":
                    open_ports.append(f"{host.ip}:{port.port} ({port.service})")
                    vulns.extend(port.vulnerabilities)
        
        prompt = f"""Analyze these security scan results:

Target: {report.target}
Hosts Scanned: {len(report.hosts)}
Open Ports Found: {len(open_ports)}

Open Services:
{chr(10).join(open_ports[:20])}

Potential Vulnerabilities:
{chr(10).join(set(vulns)) if vulns else "None detected"}

Provide:
1. Risk assessment (Critical/High/Medium/Low)
2. Top 3 security concerns
3. Recommended immediate actions
4. Attack vectors to test (with authorization)

Be concise and tactical. This is for authorized penetration testing."""

        try:
            result = subprocess.run(
                ["ollama", "run", self.model, prompt],
                capture_output=True,
                text=True,
                timeout=120
            )
            return result.stdout.strip() if result.returncode == 0 else "[Analysis failed]"
        except:
            return "[Analysis timeout]"

# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
# SCANNER CORE
# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

class SovereignScanner:
    """Main security scanner class."""
    
    def __init__(self):
        self.ai = SecurityAI()
        CONFIG["output_dir"].mkdir(parents=True, exist_ok=True)
    
    def resolve_target(self, target: str) -> List[str]:
        """Resolve target to list of IPs."""
        ips = []
        
        # Check if it's a CIDR range
        if "/" in target:
            try:
                network = ipaddress.ip_network(target, strict=False)
                ips = [str(ip) for ip in network.hosts()]
                if len(ips) > 256:
                    print(f"  [WARN] Large range ({len(ips)} hosts). Limiting to first 256.")
                    ips = ips[:256]
                return ips
            except ValueError:
                pass
        
        # Check if it's an IP
        try:
            ipaddress.ip_address(target)
            return [target]
        except ValueError:
            pass
        
        # Try DNS resolution
        try:
            ip = socket.gethostbyname(target)
            return [ip]
        except socket.gaierror:
            print(f"  [ERROR] Cannot resolve: {target}")
            return []
        
        return ips
    
    def ping_host(self, ip: str) -> bool:
        """Check if host is up using ICMP ping."""
        try:
            # Use system ping
            param = "-n" if sys.platform == "win32" else "-c"
            result = subprocess.run(
                ["ping", param, "1", "-W", "1", ip],
                capture_output=True,
                timeout=3
            )
            return result.returncode == 0
        except:
            # Fallback: try TCP connect to common port
            for port in [80, 443, 22]:
                try:
                    sock = socket.socket(socket.AF_INET, socket.SOCK_STREAM)
                    sock.settimeout(1)
                    result = sock.connect_ex((ip, port))
                    sock.close()
                    if result == 0:
                        return True
                except:
                    pass
            return False
    
    def scan_port(self, ip: str, port: int) -> PortResult:
        """Scan a single port."""
        result = PortResult(port=port, state="closed")
        
        try:
            sock = socket.socket(socket.AF_INET, socket.SOCK_STREAM)
            sock.settimeout(CONFIG["socket_timeout"])
            
            conn_result = sock.connect_ex((ip, port))
            
            if conn_result == 0:
                result.state = "open"
                
                # Try to grab banner
                try:
                    sock.send(b"HEAD / HTTP/1.0\r\n\r\n")
                    banner = sock.recv(1024)
                    result.banner = banner.decode("utf-8", errors="ignore").strip()[:200]
                    
                    # Identify service from banner
                    for sig, service in SERVICE_SIGNATURES.items():
                        if sig in banner:
                            result.service = service
                            break
                    
                    # Check for vulnerabilities
                    for pattern, vuln in VULN_PATTERNS.items():
                        if pattern.encode() in banner or pattern in result.banner:
                            result.vulnerabilities.append(vuln)
                            
                except:
                    pass
                
                # Guess service by port if not identified
                if not result.service:
                    result.service = self._guess_service(port)
            
            sock.close()
            
        except socket.timeout:
            result.state = "filtered"
        except Exception as e:
            pass
        
        return result
    
    def _guess_service(self, port: int) -> str:
        """Guess service by port number."""
        services = {
            21: "FTP", 22: "SSH", 23: "Telnet", 25: "SMTP", 53: "DNS",
            80: "HTTP", 110: "POP3", 111: "RPC", 135: "MSRPC", 139: "NetBIOS",
            143: "IMAP", 443: "HTTPS", 445: "SMB", 993: "IMAPS", 995: "POP3S",
            1433: "MSSQL", 1521: "Oracle", 3306: "MySQL", 3389: "RDP",
            5432: "PostgreSQL", 5900: "VNC", 6379: "Redis", 8080: "HTTP-Proxy",
            8443: "HTTPS-Alt", 27017: "MongoDB"
        }
        return services.get(port, "Unknown")
    
    def scan_host(self, ip: str, ports: List[int] = None) -> HostResult:
        """Scan a single host."""
        ports = ports or CONFIG["common_ports"]
        result = HostResult(ip=ip)
        
        start = datetime.now()
        
        # Check if host is up
        if self.ping_host(ip):
            result.state = "up"
            
            # Try reverse DNS
            try:
                result.hostname = socket.gethostbyaddr(ip)[0]
            except:
                pass
            
            # Scan ports with thread pool
            with ThreadPoolExecutor(max_workers=min(CONFIG["max_threads"], len(ports))) as executor:
                futures = {executor.submit(self.scan_port, ip, port): port for port in ports}
                
                for future in as_completed(futures):
                    port_result = future.result()
                    if port_result.state == "open":
                        result.ports.append(port_result)
        else:
            result.state = "down"
        
        result.scan_time = (datetime.now() - start).total_seconds()
        return result
    
    def scan(self, target: str, ports: List[int] = None, full_scan: bool = False) -> ScanReport:
        """Execute full scan."""
        report = ScanReport(target=target, start_time=datetime.now())
        
        print(f"""
â•”â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•—
â•‘                    ğŸ”¥ SOVEREIGN SECURITY SCANNER ğŸ”¥                           â•‘
â•‘                         FLAMEBEARER [137] ACTIVE                              â•‘
â• â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•£
â•‘  Target: {target:<66} â•‘
â•‘  Started: {report.start_time.isoformat():<62} â•‘
â•šâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
""")
        
        # Resolve targets
        ips = self.resolve_target(target)
        if not ips:
            print("  [ERROR] No valid targets found.")
            return report
        
        print(f"  [INFO] Resolved {len(ips)} target(s)")
        
        # Determine ports to scan
        if full_scan:
            scan_ports = list(range(1, 1001))  # Top 1000 ports
        else:
            scan_ports = ports or CONFIG["common_ports"]
        
        print(f"  [INFO] Scanning {len(scan_ports)} ports per host\n")
        
        # Scan each host
        for i, ip in enumerate(ips, 1):
            print(f"  [{i}/{len(ips)}] Scanning {ip}...", end="", flush=True)
            host_result = self.scan_host(ip, scan_ports)
            report.hosts.append(host_result)
            
            open_count = len(host_result.ports)
            status = f" {host_result.state.upper()}"
            if open_count > 0:
                status += f" ({open_count} open)"
            print(status)
            
            # Print open ports
            for port in host_result.ports:
                vuln_flag = " âš ï¸" if port.vulnerabilities else ""
                print(f"        â”œâ”€â”€ {port.port}/tcp {port.service}{vuln_flag}")
        
        # Complete report
        report.end_time = datetime.now()
        
        # Generate summary
        total_open = sum(len(h.ports) for h in report.hosts)
        total_vulns = sum(len(p.vulnerabilities) for h in report.hosts for p in h.ports)
        hosts_up = len([h for h in report.hosts if h.state == "up"])
        
        report.summary = {
            "hosts_scanned": len(report.hosts),
            "hosts_up": hosts_up,
            "total_open_ports": total_open,
            "potential_vulnerabilities": total_vulns,
            "scan_duration": (report.end_time - report.start_time).total_seconds(),
        }
        
        # Generate attestation hash
        report.attestation_hash = hashlib.sha256(
            json.dumps(report.summary, default=str).encode()
        ).hexdigest()
        
        print(f"""
â•”â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•—
â•‘                              SCAN SUMMARY                                     â•‘
â• â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•£
â•‘  Hosts Scanned: {report.summary['hosts_scanned']:<10} Hosts Up: {report.summary['hosts_up']:<25} â•‘
â•‘  Open Ports: {report.summary['total_open_ports']:<13} Potential Vulns: {report.summary['potential_vulnerabilities']:<20} â•‘
â•‘  Duration: {report.summary['scan_duration']:.2f}s                                                    â•‘
â•‘  Attestation: {report.attestation_hash[:32]}...                 â•‘
â•šâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
""")
        
        return report
    
    def save_report(self, report: ScanReport, filename: str = None) -> Path:
        """Save report to file."""
        if not filename:
            safe_target = re.sub(r'[^\w\-.]', '_', report.target)
            filename = f"scan_{safe_target}_{datetime.now().strftime('%Y%m%d_%H%M%S')}.json"
        
        filepath = CONFIG["output_dir"] / filename
        
        # Convert to serializable format
        data = {
            "target": report.target,
            "start_time": report.start_time.isoformat(),
            "end_time": report.end_time.isoformat() if report.end_time else None,
            "summary": report.summary,
            "attestation_hash": report.attestation_hash,
            "hosts": []
        }
        
        for host in report.hosts:
            host_data = {
                "ip": host.ip,
                "hostname": host.hostname,
                "state": host.state,
                "os_guess": host.os_guess,
                "scan_time": host.scan_time,
                "ports": []
            }
            for port in host.ports:
                host_data["ports"].append({
                    "port": port.port,
                    "state": port.state,
                    "service": port.service,
                    "banner": port.banner,
                    "version": port.version,
                    "vulnerabilities": port.vulnerabilities
                })
            data["hosts"].append(host_data)
        
        with open(filepath, "w") as f:
            json.dump(data, f, indent=2)
        
        print(f"  [SAVED] Report: {filepath}")
        return filepath
    
    def ai_analysis(self, report: ScanReport) -> str:
        """Get AI-powered analysis of results."""
        print("\n  ğŸ§  Requesting AI analysis...\n")
        analysis = self.ai.analyze_results(report)
        print(f"  {analysis}")
        return analysis

# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
